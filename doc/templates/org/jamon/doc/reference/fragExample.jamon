<%def _lilink>
  <%frag content/>
  <%args>
    String href;
  </%args>
  <li><a href="<% href %>"><& content &></a></li>\
</%def>

<%def _big>
  <%frag stuff/>
<font size="+4"><& stuff &></font>\
</%def>

<ul>
  <&| _lilink; href => "index.html" &>Home</&>
  <&| _lilink; href => "faq.html" &><b>FAQ</b></&>
  <&| _lilink; href => "toc.html" &><&| _big &>Table of contents</&></&>
</ul>

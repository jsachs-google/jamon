<%args>
  String number;
  String separator = ".";
</%args>\
<%if number.length() == 7 %>\
<% number.substring( 0, 3 ) %><% separator %><% number.substring( 3 ) %>\
<%elseif number.length() == 10 %>\
<% number.substring( 0, 3 ) %><% separator %>\
<% number.substring( 3, 6 ) %><% separator %><% number.substring( 6 ) %>\
<%else><% number %></%if>
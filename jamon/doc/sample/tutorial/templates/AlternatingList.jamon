  <%import>
    java.util.Iterator;
  </%import>
  <%frag content>
    Object renderable;
  </%frag>
  <%args>
    Iterator items;
  </%args>
% int row = 0;
  <table>
% while (items.hasNext())
% {
    <tr bgcolor="<% row++ % 2 == 0 ? "white" : "grey" %>">
      <& content; renderable => items.next() &>
    </tr>
% }
  </table>

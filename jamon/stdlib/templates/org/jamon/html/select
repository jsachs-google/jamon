<%farg renderOption>
  Object renderable;
</%farg>
<%args>
  org.jamon.html.Select input;
  boolean multiple => false;
</%args>
<select name="<% input.getName() %>" \
% if (multiple) {
  multiple \
% }
>
% for (java.util.Iterator i = input.getValues(); i.hasNext(); ) {
%   Object value = i.next();
  <option value="<% value %>" \
% if (input.isSelected(value)) {
  selected \
% }
  ><& renderOption; renderable => input.getRenderable(value) &>
% }
</select>
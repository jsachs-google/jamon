<%farg content></%farg>\
<%args>
  Escaping escaping;
  boolean compose;
</%args>\
<%import>
  java.io.Writer;
  java.io.StringWriter;
  org.jamon.Escaping;
</%import>\
<%java>
  content.escaping(compose ? escaping : getEscaping());
  String text = content.makeRenderer().asString();
  escaping(escaping);
</%java>\
<% text %>